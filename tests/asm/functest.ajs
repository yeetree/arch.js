! This program tests functions by defining a function that prints 'B'

label loop; ! Defines our loop

outb 'A' 0; ! Print 'A' to serial console

call :sayb; ! Call our function

outb 'C' 0; ! Print 'C"

jmp :loop; ! Jump back to loop

label sayb; ! Define our function

outb 'B' 0; ! Print 'B'

ret; ! Make sure to return